{
  "name": "@projectlibertylabs/siwf-utils",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "main": "./dist/fcp-utils.umd.cjs",
  "module": "./dist/fcp-utils.js",
  "exports": {
    ".": {
      "import": "./dist/fcp-utils.js",
      "require": "./dist/fcp-utils.cjs"
    }
  },
  "types": "./dist/fcp-utils.d.ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "test": "vitest run --silent",
    "test:loud": "vitest run",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@projectlibertylabs/eslint-config-siwf": "workspace:*",
    "@projectlibertylabs/siwf": "workspace:*",
    "@vitest/coverage-v8": "3.2.4",
    "jsdom": "^25.0.1",
    "pathe": "^1.1.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "7.1.5",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@frequency-chain/api-augment": "1.17.6-rc1",
    "@polkadot/api": "16.4.8",
    "@polkadot/extension-dapp": "^0.61.1",
    "@polkadot/extension-inject": "^0.61.1",
    "@polkadot/types": "16.4.8",
    "@polkadot/util": "13.5.7",
    "@polkadot/util-crypto": "13.5.7",
    "@stablelib/random": "^2.0.0"
  }
}
