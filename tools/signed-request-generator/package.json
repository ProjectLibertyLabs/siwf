{
	"name": "signed-request-generator",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"generate:siwf": "cd ../../libraries/js && npm i && npm run build && cd dist && npm pack",
		"dev": "vite dev",
		"build": "vite build",
		"build:copy": "vite build && rm -Rf ../../docs/src/Generator && cp -a build ../../docs/src/Generator",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@polkadot/extension-dapp": "^0.59.1",
		"@polkadot/extension-inject": "^0.59.1",
		"@projectlibertylabs/siwf": "file:../../libraries/js/dist/projectlibertylabs-siwf-0.0.0.tgz"
	},
	"devDependencies": {
		"@frequency-chain/style-guide": "^0.3.0",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.21.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.1",
		"globals": "^16.2.0",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.33.14",
		"svelte-check": "^4.2.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.1",
		"vite": "^6.3.5",
		"vite-plugin-singlefile": "^2.2.0"
	},
	"type": "module"
}
