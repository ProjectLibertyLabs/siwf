{
	"name": "signed-request-generator",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"generate:siwf": "cd ../../libraries/js && npm i && npm run build && cd dist && npm pack",
		"dev": "vite dev",
		"build": "vite build",
		"build:copy": "vite build && rm -Rf ../../docs/src/Generator && cp -a build ../../docs/src/Generator",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@frequency-chain/ethereum-utils": "2.0.0",
		"@polkadot/extension-dapp": "0.61.1",
		"@polkadot/extension-inject": "0.61.1",
		"@projectlibertylabs/siwf": "file:../../libraries/js/dist/projectlibertylabs-siwf-0.0.0.tgz"
	},
	"devDependencies": {
		"@frequency-chain/style-guide": "0.4.1",
		"@playwright/test": "1.57.0",
		"@sveltejs/adapter-auto": "7.0.1",
		"@sveltejs/adapter-static": "3.0.10",
		"@sveltejs/kit": "2.22.2",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@types/eslint": "9.6.1",
		"autoprefixer": "10.4.21",
		"eslint": "9.30.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.15.0",
		"globals": "16.3.0",
		"postcss": "8.5.6",
		"prettier": "3.8.1",
		"prettier-plugin-svelte": "3.5.0",
		"svelte": "5.53.0",
		"svelte-check": "4.2.2",
		"typescript": "5.8.3",
		"typescript-eslint": "8.34.0",
		"vite": "7.1.11",
		"vite-plugin-singlefile": "2.3.0"
	},
	"type": "module"
}
